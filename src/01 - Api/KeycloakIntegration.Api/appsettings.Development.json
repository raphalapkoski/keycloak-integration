{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
    "base-url": "http://localhost:8080",
    "metadata-address-url": "http://localhost:8080/realms/keycloak/.well-known/openid-configuration",
    "authority-server-url": "http://localhost:8080/realms/keycloak/protocol/openid-connect/auth",
    "require-https-metadata": false,
    "validate-token-audience": true,
    "validate-token-lifetime": true,
    "audience": "keycloak"
  }
}
